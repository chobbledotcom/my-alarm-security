{
  "name": "importer-tests",
  "version": "1.0.0",
  "description": "Test suite for site importer",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "converters/**/*.js",
      "utils/**/*.js",
      "!**/node_modules/**"
    ]
  },
  "devDependencies": {
    "jest": "^29.0.0"
  }
}
