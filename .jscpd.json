{
  "threshold": 0,
  "reporters": ["console", "json"],
  "output": ".jscpd-report",
  "ignore": [
    "**/node_modules/**",
    "**/_site/**",
    "**/.build/**",
    "**/packages/**"
  ],
  "format": ["javascript"],
  "absolute": false,
  "gitignore": true,
  "exitCode": 1,
  "minLines": 1,
  "minTokens": 40,
  "path": ["scripts"]
}
